entity employee {
 *emp_id: integer
  emp_name: text
  job_name: text
  manager (manager_id): employee
  hire_date: date
  salary: currency
  commission: text
  department (dep_id): department
}

entity department {
 *dep_id: integer
  dep_name: text
  dep_location: text
}

entity salary_grade {
 *grade: integer
  min_sal: decimal
  max_sal: decimal
}
